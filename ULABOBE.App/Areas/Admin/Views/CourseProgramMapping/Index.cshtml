@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<div class="row">
    <div class="col-6">
        <h2 class="text-primary">Course-Program Mapping List </h2>
    </div>
    <div class="col-6 text-right">
        <a class="btn btn-primary" asp-action="Upsert"><i class="fas fa-plus"></i> &nbsp; Create New Course-Program Mapping</a>
    </div>
</div>

<br />
<div class="p-4 border rounded">
    <table id="tblData" class="table table-striped table-bordered" style="width:100%">
        <thead class="thead-dark">
            <tr class="table-info">
                <th>Course Info</th>
                <th>Semester</th>
                <th>Course Learning</th>
                <th>PLO</th>
                <th>GS</th>
                <th>PS</th>
                <th>SDG</th>
                <th>AR</th>
                <th>Is-Approved</th>
                <th>Action</th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts{

    <script src="~/js/courseProgramMapping.js"></script>
}